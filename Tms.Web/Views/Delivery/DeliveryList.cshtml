        <!-- **************
           RIGHT SLIDE STARTS
           **************** -->
        <div id="right_slide_div" class="row" style="display:none;">
            <div class="col-md-12 content_grey">
                <div class="clear"></div>
                <div class="box-typical-header">
                    <div class="cell_title">
                        <h3>Delivery</h3>
                    </div>
                    <div class="cell_actions">
                        <button class="action_btn red" type="button" id="right_close">
                            <i class="fa fa-close" aria-hidden="true"></i>
                        </button>
                    </div>
                </div>
                <div class="clear"></div>
                <div class="col-md-12">
                    <div class="box_section border_blue">
                        <h2 class="box_title">Delivery Description</h2>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Delivery No</label>
                            <input type="text" id="" class="form-control">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Delivery Type</label>
                            <select class="form-control" id="">
                                <option value="0">-Select</option>
                                <option value="1">Bulk</option>
                                <option value="2">Individual</option>
                            </select>
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Status</label>
                            <select class="form-control" id="">
                                <option value="1">Completed</option>
                                <option value="2">Pending</option>
                                <option value="3">Returned</option>
                            </select>
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Self Delivered</label>
                            <div class="clear"></div>
                            <label class="radio-inline">
                                <input type="radio" value="option1" id="" name="delivery"> Yes
                            </label>
                            <label class="radio-inline">
                                <input type="radio" value="option2" id="" name="delivery"> No
                            </label>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                    <div class="box_section border_green">
                        <h2 class="box_title">Delivery Details</h2>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Consignee ID</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Consignee Name</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="col-sm-6 col-md-6 form-group">
                            <label class="control-label">Consignee Address</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="clear"></div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Origin</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">MAWN No</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">HAWB No</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Carrier/Flight No</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="clear"></div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Arrival Date</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">No of Packages</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Weight</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Cargo Contents</label>
                            <input type="text" class="form-control" id="">
                        </div>
                        <div class="clear"></div>
                        <div class="col-sm-12 col-md-12 form-group">
                            <label class="control-label">Delivery Instructions</label>
                            <textarea class="form-control" rows="1" id=""></textarea>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                    <div class="box_section border_red">
                        <h2 class="box_title">Deliver To Customer<a href="javascript:void(0)" class="add_row" id="add_contact_row"><i class="fa fa-plus" aria-hidden="true"></i></a></h2>


                        <div class="col-sm-12 col-md-12 form-group">
                            <div class="table-responsive">
                                <table class="table table-striped" id="partner_contact_details">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Address</th>
                                            <th>City</th>
                                            <th>Zip</th>
                                            <th>Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td><input type="text" id="" class="form-control"></td>
                                            <td><input type="text" id="" class="form-control"></td>
                                            <td><input type="text" id="" class="form-control"></td>
                                            <td><input type="text" id="" class="form-control"></td>
                                            <td><i class="fa fa-floppy-o"></i><i class="fa fa-trash"></i></td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="clear"></div>
                            </div>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                    <div class="box_section border_skyblue">
                        <h2 class="box_title">Additional Information</h2>
                        <div class="col-sm-12 col-md-12 form-group">
                            <textarea class="form-control" rows="2" id=""></textarea>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
                <div class="row">
                    <div class="form-group">
                        <div class="col-md-6 pull-right">
                            <input type="reset" href="javascript:void(0)" id="right_close" class="btn wt_btn pull-right" value="Cancel">
                            <input type="button" href="javascript:void(0)" id="si_save" class="btn def_btn pull-right" value="Save Changes">
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <!-- **************
           EOF RIGHT SLIDE
           **************** -->
        <div class="clear"></div>
    </div>
</div>

<script>
    $(function () {
        var activeclass = $('.sidebar-menu .has-sub .sub li > a[href="/Tms/Delivery/DeliveryList"]');

        $(activeclass).parent().parent().parent().addClass("active");
        $(activeclass).parent().addClass('current');
    });


    /* *****  Add Contact Row ***** */
    var counter = 1;
    jQuery('a#add_contact_row').click(function(event){
        event.preventDefault();
        counter++;

        
        var newContactRow = jQuery('<tr><td><input type="text" id="" class="form-control"></td>'+
                             counter + '<td><input type="text" id="" class="form-control"></td>'+
                             counter + '<td><input type="text" id="" class="form-control"></td>'+
                             counter + '<td><input type="text" id="" class="form-control"></td>'+
                             counter + '<td><i class="fa fa-floppy-o"></i><i class="fa fa-trash"></i></td></tr>');
                                    
        jQuery('#partner_contact_details').append(newContactRow);
    });

    /* *****  Upload Doc ***** */
    $("#upload_doc").click(function() {
        $("input[id='my_file']").click();
    });

</script>