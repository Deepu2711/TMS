@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="main-content">
    <div id="content" class="row content_grey">
        <div id="right_content" class="row">
            <div class="box-typical-header">
                <div class="cell_title">
                    <h3>Resource Avilibility</h3>
                </div>
                <div class="cell_actions">
                    <button class="action_btn btn_calendar" type="button" id="" style="border-right: 1px solid #dcdcdc;padding-right: 10px;">
                        <i class="fa fa-calendar" aria-hidden="true" title="Calendar View"></i>
                    </button>
                    <button class="action_btn btn_grid" type="button" id="" style="border-right: 1px solid #dcdcdc;padding-right: 10px;display:none;">
                        <i class="fa fa-th" aria-hidden="true" title="Grid View"></i>
                    </button>
                    <button class="action_btn" type="button" id="right_slide_button">
                        <i class="fa fa-plus" aria-hidden="true" title="Add"></i>
                    </button>

                </div>
            </div>
            <div class="clear"></div>
            <div class="col-md-12">
                <!-- Table -->
                <div class="table-responsive box_section border_blue show_table_resource">
                    <table id="data_table" class="table table-striped display">
                        <thead>
                            <tr>
                                <th>Type</th>
                                <th>Name/Vehicle No./Serial No.</th>
                                <th>From Date & time</th>
                                <th>To Date & time</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Vehicle</td>
                                <td>Heavy Truck</td>
                                <td>23-10-2015 10:00</td>
                                <td>23-10-2015 15:00</td>
                                <td>
                                    <i class="pull_right_slide_view fa fa-edit"></i>
                                    <i class="fa fa-trash"></i>
                                </td>
                            </tr>
                            <tr>
                                <td>Manpower</td>
                                <td>Rocco</td>
                                <td>21-10-2015 10:00</td>
                                <td>23-10-2015 15:00</td>
                                <td>
                                    <i class="pull_right_slide_view fa fa-edit"></i>
                                    <i class="fa fa-trash"></i>
                                </td>
                            </tr>
                            <tr>
                                <td>Equipment</td>
                                <td>MT3423</td>
                                <td>23-10-2015 10:00</td>
                                <td>23-10-2015 15:00</td>
                                <td>
                                    <i class="pull_right_slide_view fa fa-edit"></i>
                                    <i class="fa fa-trash"></i>
                                </td>
                            </tr>
                            <tr>
                                <td>Manpower</td>
                                <td>Hit Man</td>
                                <td>21-10-2015 10:00</td>
                                <td>23-10-2015 15:00</td>
                                <td>
                                    <i class="pull_right_slide_view fa fa-edit"></i>
                                    <i class="fa fa-trash"></i>
                                </td>
                            </tr>
                            <tr>
                                <td>Vehicle</td>
                                <td>Heavy Truck</td>
                                <td>23-10-2015 10:00</td>
                                <td>23-10-2015 15:00</td>
                                <td>
                                    <i class="pull_right_slide_view fa fa-edit"></i>
                                    <i class="fa fa-trash"></i>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
                <!-- Calender -->
                <div class="box_section border_blue show_calendar_view" style="display:none;">
                    <h2 class="box_title">
                        <span id="cal_manpower_title">Manpower</span>
                        <span id="cal_vehicle_title">Vehicles</span>
                        <span id="cal_equipment_title">Equipment</span>
                        <div class="col-sm-3 col-md-3 form-group pull-right" style="margin-bottom: 0px;">
                            <select class="form-control" id="sel_cal_all">
                                <option value="man_power" selected>Man Power</option>
                                <option value="vehicles">Vehicles</option>
                                <option value="equipments">Equipments</option>
                            </select>
                        </div>
                    </h2>
                    <div id='calendar_manpower'></div>
                    <div id='calendar_vehicles'></div>
                    <div id='calendar_equipment'></div>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
            </div>

            <div class="clear"></div>
        </div>
        <div class="clear"></div>
        <!-- **************
           RIGHT SLIDE STARTS
           **************** -->
        <div id="right_slide_div" class="row" style="display:none;">
            <div class="col-md-12 content_grey">
                <div class="clear"></div>
                <div class="box-typical-header">
                    <div class="cell_title">
                        <h3>Resource Avilibility</h3>
                    </div>
                    <div class="cell_actions">
                        <button class="action_btn red" type="button" id="right_close">
                            <i class="fa fa-close" aria-hidden="true"></i>
                        </button>
                    </div>
                </div>
                <div class="clear"></div>
                <div class="col-md-12">
                    <div class="box_section border_blue">
                        <h2 class="box_title">Resource</h2>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">Type</label>
                            <select class="form-control sel_resource" id="">
                                <option value="0">-Select-</option>
                                <option value="1">Equipment</option>
                                <option value="2">Manpower</option>
                                <option value="3">Vehicles</option>
                            </select>
                        </div>
                        <div class="col-sm-3 col-md-3 form-group veh_no" style="display:none;">
                            <label class="control-label">Vehicle No.</label>
                            <select class="form-control" id="">
                                <option value="1">-Select-</option>
                                <option value="2">HT1223</option>
                                <option value="3">MT09277</option>
                                <option value="4">LT8573</option>
                                <option value="5">MT4873</option>
                            </select>
                        </div>
                        <div class="col-sm-3 col-md-3 form-group name_res" style="display:none;">
                            <label class="control-label">Name</label>
                            <select class="form-control" id="">
                                <option value="1">-Select-</option>
                                <option value="2">HT1223</option>
                                <option value="3">MT09277</option>
                                <option value="4">LT8573</option>
                                <option value="5">MT4873</option>
                            </select>
                        </div>
                        <div class="col-sm-3 col-md-3 form-group ser_no" style="display:none;">
                            <label class="control-label">Serial No.</label>
                            <select class="form-control" id="">
                                <option value="1">-Select-</option>
                                <option value="2">HT1223</option>
                                <option value="3">MT09277</option>
                                <option value="4">LT8573</option>
                                <option value="5">MT4873</option>
                            </select>
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">From Date & Time</label>
                            <input type="text" class="form-control">
                        </div>
                        <div class="col-sm-3 col-md-3 form-group">
                            <label class="control-label">To Date  & Time</label>
                            <input type="text" class="form-control">
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                    <div class="box_section border_skyblue">
                        <h2 class="box_title">Additional Information</h2>
                        <div class="col-sm-12 col-md-12 form-group">
                            <textarea class="form-control" rows="2" id=""></textarea>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
                <div class="row">
                    <div class="form-group">
                        <div class="col-md-6 pull-right">
                            <input type="reset" href="javascript:void(0)" id="right_close" class="btn wt_btn pull-right" value="Cancel">
                            <input type="button" href="javascript:void(0)" id="si_save" class="btn def_btn pull-right" value="Save Changes">
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <!-- **************
           EOF RIGHT SLIDE
           **************** -->
    </div>
</div>

<script>
    $(function () {
        var activeclass = $('.sidebar-menu .has-sub .sub li > a[href^="/Tms/ResourceAvailablity/ResourceAvailablityList"]');

        $(activeclass).parent().parent().parent().addClass("active");
        $(activeclass).parent().addClass('current');
    });
</script>
<script type="text/javascript" src="~/Scripts/ResourceAvalibility.js"></script>